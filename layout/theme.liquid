<!DOCTYPE html>
<html>
  <head>
    <meta name="viewport" content= "width=device-width, initial-scale=1.0">
    {{ content_for_header }}
    {% comment %} {{- 'bootstrap.min.css' | asset_url | stylesheet_tag -}} {% endcomment %}
    {% comment %} {{- 'styles.css' | asset_url | stylesheet_tag -}} {% endcomment %}
    {{- 'theme.css ' | asset_url | stylesheet_tag -}}

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

  </head>
  <body>
    {%- section 'header' -%}
    {{ content_for_layout }}
    {% comment %} {%- section 'footer' -%} {% endcomment %}
    {% comment %} {{- 'jquery-3.1.1.min.js' | asset_url | script_tag -}}
    {{- 'bootstrap.min.js' | asset_url | script_tag -}} {% endcomment %}
    {{- 'theme.js' | asset_url | script_tag -}}
  </body>
</html>
